{
    "name": "houseoftech/laravel-recurring",
    "description": "simshaun/recurr integration for Laravel/Eloquent.",
    "keywords": ["laravel", "framework", "Laravel-Recurring", "Laravel Recurring"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    },
    {
        "name": "Corie Slate"
    }],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "simshaun/recurr": "^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Recurring\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Recurring\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
		"laravel": {
			"providers": [
				"BrianFaust\\Recurring\\LaravelRecurringServiceProvider"
			]
		}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
